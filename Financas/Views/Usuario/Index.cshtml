@model IList<Financas.Entidades.Usuario>
@Html.ActionLink("Novo Usuário", "Form", "Usuario")
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>E-mail</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var usuario in Model)
    {
        <tr>
            <td>@usuario.UsuarioID</td>
            <td>@usuario.Nome</td>
            <td>@usuario.Email</td>
        </tr>
        

    }
    </tbody>
</table>